<odoo>

        <template id="lab_det">
        	<t t-call="website.layout">
            <title>JS</title>
            <div class="container">
            <a role="button" class="btn btn-primary my-1" t-attf-href="/lab/create/">Create</a>
            <table border="1px solid" width="100%">
            	<tr>
            		<td>state</td>
            		<td>state</td>
            		<td>state</td>
            		<td>state</td>
            		<td>state</td>
            		<td>state</td>
            		<td>state</td>
            		<td>state</td>
            		<td>state</td>
            		<td>state</td>
            		<td>Action</td>
            	</tr>
            	
		            <t t-foreach="status" t-as="st">
		            	<tr>
		              <td><t t-esc="st.name"/></td>
		              <td><t t-esc="st.labaddress"/></td>
		              <td><t t-esc="st.labcontact"/></td>
		              <td><t t-esc="st.labemail"/></td>
		              <td><t t-esc="st.city"/></td>
		              <td><t t-esc="st.district"/></td>
		              <td><t t-esc="st.state"/></td>
		              <td><t t-esc="st.pincode"/></td>
		              <td><t t-esc="st.facilityname"/></td>
		              <td><t t-esc="st.price"/></td>
		              <td>
		              	<a t-attf-href="/lab/edit/{{ slug(st) }}" ><i class="fa fa-pencil"/></a>
		              	<a t-attf-href="/lab/delete/{{ slug(st) }}"><i class="fa fa-trash"/></a>
		              </td>
		            </tr>
	        	</t>
	        </table>
	    </div>
	    </t>
        </template>

        <template id="lab_cre">
            <t t-call="website.layout">
                <title>JS</title>
                <div class="container">
                    <form t-attf-action="/lab/created/#{labs.id if labs else ''}" id="lab_details_form" method="post">
                        <div class="form-group">
                            <label for="name">Lab Name</label>
                            <input type="text" class="form-control" name="name" placeholder="JS Lab Pvt. Ltd." t-attf-value="#{labs.name if labs else ''}"/>
                        </div>
                        <div class="form-group">
                            <label for="labaddress">Laboratory Address</label>
                            <input type="text" class="form-control" name="labaddress" placeholder="06, Snow Street, Mumbai." t-attf-value="#{labs.labaddress if labs else ''}"/>
                        </div>
                        <div class="form-group">
                            <label for="labcontact">Contact No.</label>
                            <input type="number" class="form-control" name="labcontact" placeholder="8153696906" t-attf-value="#{labs.labcontact if labs else ''}"/>
                        </div>
                        <div class="form-group">
                            <label for="labemail">E-Mail Address</label>
                            <input type="email" class="form-control" name="labemail" placeholder="jslab@gmail.com" t-attf-value="#{labs.labemail if labs else ''}"/>
                        </div>
                        <div class="form-group">
                            <label for="city">City</label>
                            <input type="text" class="form-control" name="city" placeholder="Juhu" t-attf-value="#{labs.city if labs else ''}"/>
                        </div>
                        <div class="form-group">
                            <label for="district">District</label>
                            <input type="text" class="form-control" name="district" placeholder="Mumbai" t-attf-value="#{labs.district if labs else ''}"/>
                        </div>
                        <div class="form-group">
                            <label for="state">State</label>
                            <input type="text" class="form-control" name="state" placeholder="Maharashtra" t-attf-value="#{labs.state if labs else ''}"/>
                        </div>
                        <div class="form-group">
                            <label for="pincode">Pincode</label>
                            <input type="number" class="form-control" name="pincode" placeholder="400049" t-attf-value="#{labs.pincode if labs else ''}"/>
                        </div>
                        <div class="form-group">
                            <label for="facilityname">Laboratory Facility</label>
                            <select class="form-control" name="facilityname"  t-attf-value="#{labs.facilityname if labs else ''}">
                                <option value="soiltest">Soil Test</option>
                                <option value="concretetest">Concrete Test</option>
                                <option value="cementtest">Cement Test</option>
                                <option value="steeltest">Steel Test</option>
                                <option value="fieldtest">Field Test</option>
                            </select>
                        </div>
                        <div class="form-group">
                            <label for="price">Price</label>
                            <input type="number" class="form-control" name="price" placeholder="10000" t-attf-value="#{labs.price if labs else ''}"/>
                        </div>
                        <div class="form-group">
                            <button type="submit" class="btn btn-primary my-1 form-control">Submit</button>
                        </div>
                    </form>
                </div>
            </t>
        </template>
        
</odoo>